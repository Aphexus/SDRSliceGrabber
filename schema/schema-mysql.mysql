CREATE TABLE FILES (
    FILE_ID INT UNSIGNED AUTO_INCREMENT PRIMARY KEY,
    ASSET_CLASS ENUM('RATES','FOREX','EQUITIES','COMMODITIES','CREDITS') NOT NULL,
    FILE_NAME CHAR(38) NOT NULL,
    UPLOAD_TIMESTAMP TIMESTAMP NOT NULL,
    DATE_STR CHAR(10) NOT NULL
);

CREATE TABLE FOREX (
    FILE_ID INT UNSIGNED,
    CANCELED_BY INT UNSIGNED,
    CORRECTED_BY INT UNSIGNED,
    PROCESSED BOOLEAN,
    DISSEMINATION_ID INT UNSIGNED,
    ORIGINAL_DISSEMINATION_ID INT UNSIGNED,
    ACTION VARCHAR(7),
    EXECUTION_TIMESTAMP DATETIME,
    CLEARED CHAR(1),
    INDICATION_OF_COLLATERALIZATION CHAR(2),
    INDICATION_OF_END_USER_EXCEPTION CHAR(1),
    INDICATION_OF_OTHER_PRICE_AFFECTING_TERM CHAR(1),
    BLOCK_TRADES_AND_LARGE_NOTIONAL_OFF_FACILITY_SWAPS CHAR(1),
    EXECUTION_VENUE CHAR(3),
    EFFECTIVE_DATE DATE,
    END_DATE DATE,
    DAY_COUNT_CONVENTION VARCHAR(15),
    SETTLEMENT_CURRENCY VARCHAR(5),
    ASSET_CLASS CHAR(2),
    SUB_ASSET_CLASS_FOR_OTHER_COMMODITY VARCHAR(30),
    TAXONOMY VARCHAR(120),
    PRICE_FORMING_CONTINUATION_DATA VARCHAR(50),
    UNDERLYING_ASSET_1 VARCHAR(120),
    UNDERLYING_ASSET_2 VARCHAR(120),
    PRICE_NOTATION_TYPE VARCHAR(30),
    PRICE_NOTATION VARCHAR(30),
    ADDITIONAL_PRICE_NOTATION_TYPE VARCHAR(30),
    ADDITIONAL_PRICE_NOTATION VARCHAR(30),
    NOTIONAL_CURRENCY_1 VARCHAR(12),
    NOTIONAL_CURRENCY_2 VARCHAR(12),
    ROUNDED_NOTIONAL_AMOUNT_1 VARCHAR(30),
    ROUNDED_NOTIONAL_AMOUNT_2 VARCHAR(30),
    PAYMENT_FREQUENCY_1 VARCHAR(12),
    PAYMENT_FREQUENCY_2 VARCHAR(12),
    RESET_FREQUENCY_1 VARCHAR(12),
    RESET_FREQUENCY_2 VARCHAR(12),
    EMBEDED_OPTION VARCHAR(12),
    OPTION_STRIKE_PRICE VARCHAR(30),
    OPTION_TYPE VARCHAR(5),
    OPTION_FAMILY VARCHAR(5),
    OPTION_CURRENCY VARCHAR(5),
    OPTION_PREMIUM VARCHAR(30),
    OPTION_LOCK_PERIOD DATE,
    OPTION_EXPIRATION_DATE DATE,
    PRICE_NOTATION2_TYPE VARCHAR(30),
    PRICE_NOTATION2 VARCHAR(30),
    PRICE_NOTATION3_TYPE VARCHAR(30),
    PRICE_NOTATION3 VARCHAR(30),
    FOREIGN KEY(FILE_ID) REFERENCES FILES(FILE_ID),
    PRIMARY KEY(DISSEMINATION_ID)
);

